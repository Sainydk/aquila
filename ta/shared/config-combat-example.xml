<!-- Example -->
<Config>
    <Database>
        <driverClass>com.mysql.jdbc.Driver</driverClass>
        <url>jdbc:mysql://192.168.1.5/quik_export</url>
        <user>root</user>
        <password>root</password>
    </Database>
    
    <MarketData type="jdbc" table="rts_05m" column="period_time" />
    
    <Assets type="quik" assets="[export]assets" deals="[export]allDeals" />

    <Properties>
    	<Account>SPBFUT00eqe</Account>
    	<Asset>RIH2</Asset>
    
        <StrategyClass>ru.prolib.aquila.ChaosTheory.ChaosTheoryImpl4</StrategyClass>
        <RiskManagerClass>ru.prolib.aquila.ChaosTheory.RiskManagerSimpleLimit</RiskManagerClass>
        <RiskManager.MaxPos>2</RiskManager.MaxPos>
        <EmergTimeout>12000</EmergTimeout>
        <UpdateSignal>rts_05m</UpdateSignal>
    </Properties>
    
    <PortfolioState type="quik" asset="RIH2"
    	position="[export]position" limits="[export]limits" />

    <Export topic="[export]allDeals">
        <Asset code="RIH2">
            <Writer period="5" table="rts_05m"/>
            <Writer period="1" table="rts_1m"/>
        </Asset>
    </Export>
    
    <Portfolio type="quik">
        <OutputFile>/home/whirlwind/VirtualBox/shared/quik-export/transaction.tri</OutputFile>
        <ResultFile>/home/whirlwind/VirtualBox/shared/quik-export/transaction.tro</ResultFile>
    </Portfolio>
</Config>
