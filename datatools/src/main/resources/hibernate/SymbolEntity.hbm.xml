<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ru.prolib.aquila.datatools.storage.model.SymbolEntity" table="symbols">
		<id name="id" access="field">
			<generator class="increment" />
		</id>
		<property name="symbol" unique="true" access="field" not-null="true"
			type="ru.prolib.aquila.datatools.storage.dao.HibernateSymbolType" >
			<column name="code" length="16" not-null="true" />
			<column name="class_code" length="16" not-null="true" />
			<column name="currency" length="3" not-null="true" />
			<column name="type" length="4" not-null="true" />
		</property>
	</class>
</hibernate-mapping>