<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ru.prolib.aquila.probe.storage.model.TradingSessionPropertiesEntity" table="trading_session_properties">
		<id name="id">
			<generator class="increment" />
		</id>
		<many-to-one name="symbol" class="ru.prolib.aquila.probe.storage.model.SymbolEntity" column="symbol" unique="true" />
		<property name="scale" column="scale" not-null="true" />
		<property name="tickCost" column="tick_cost" not-null="true" />
		<property name="initialMarginCost" column="initial_margin_cost" not-null="true" />
		<property name="initialPrice" column="initial_price" not-null="true" />
		<property name="lowerPriceLimit" column="lower_price_limit" not-null="true" />
		<property name="upperPriceLimit" column="upper_price_limit" not-null="true" />
		<property name="lotSize" column="lot_size" not-null="true" />
		<property name="tickSize" column="tick_size" not-null="true" />
		<property name="snapshotTime" column="snapshot_time" not-null="true"
			type="ru.prolib.aquila.probe.storage.dao.DateTimeType" />
		<property name="clearingTime" column="clearing_time" not-null="true"
			type="ru.prolib.aquila.probe.storage.dao.DateTimeType" />
	</class>
</hibernate-mapping>